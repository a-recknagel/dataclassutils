{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        }
    },
    "handlers": {
        "std_out_stream": {
            "class": "logging.StreamHandler",
            "formatter": "standard",
            "level": "INFO"
        }
    },
    "loggers": {
        "": {
            "level": "INFO",
            "propagate": true,
            "handlers": [
                "std_out_stream"
            ]
        }
    }
}
